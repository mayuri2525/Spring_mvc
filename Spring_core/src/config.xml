<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


<bean id="a" class="com.hefshine.util.AddressModel">
<property name="city" value="pune"></property>
<property name="state" value="MH"></property>
<property name="country" value="india"></property>
</bean>
<bean id="e1" class="com.hefshine.util.Employee">

<constructor-arg name="id" value="20"></constructor-arg>
<constructor-arg name="name" value="xyc"></constructor-arg>
<constructor-arg name="add" ref="a"></constructor-arg>

</bean>
</beans>


